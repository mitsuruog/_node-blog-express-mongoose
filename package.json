{
  "name": "node-sample-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "postinstall": "bower install",
    "import-users": "mongoimport --host 192.168.99.100 --db blog --collection users --file config/seeds/users.json --jsonArray",
    "import-articles": "mongoimport --host 192.168.99.100 --db blog --collection articles --file config/seeds/articles.json --jsonArray"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "bower": "^1.6.3",
    "connect-redis": "^3.0.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.11.3",
    "jade": "~1.11.0",
    "mongodb": "^2.0.46",
    "mongoose": "^4.1.12",
    "morgan": "~1.6.1",
    "passport": "^0.3.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.0"
  },
  "cacheDirectories": [
    "node_modules",
    "bower_components"
  ]
}
